    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
