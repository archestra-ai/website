    "knip": "^5.70.2",
    "postcss": "^8.5",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vite": "^7.2.4",
    "vitest": "^4.0.13"
