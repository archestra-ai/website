    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "swagger-ui-react": "^5.30.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.13",
    "zod-to-json-schema": "^3.25.0"
